[run]
source = src
branch = True
omit =
    */__pycache__/*
    */.venv/*
    */tests/*
    */venv/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

show_missing = True
precision = 2

[html]
directory = htmlcov

[xml]
output = coverage.xml
